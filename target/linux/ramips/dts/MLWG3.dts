/dts-v1/;

/include/ "mt7628an.dtsi"

/ {
	compatible = "mlwg3", "mediatek,mt7628an-soc";
	model = "Kingston MLWG3";

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x4000000>;
	};

	pinctrl {
		state_default: pinctrl0 {
			gpio {
				ralink,group = "refclk", "gpio";
				ralink,function = "gpio";
			};
		};
	};

	wmac@10300000 {
		status = "okay";
	};

	sdhci@10130000 {
	        compatible = "ralink,mt7620-sdhci";
		reg = <0x10130000 4000>;

		interrupt-parent = <&intc>;
		interrupts = <14>;

		status = "okay";
	};

	palmbus@10000000 {
		spi@b00 {
			status = "okay";

			m25p80@0 {
				#address-cells = <1>;
				#size-cells = <1>;
				compatible = "jedec,spi-nor";
				reg = <0 0>;
				linux,modalias = "m25p80";
				spi-max-frequency = <10000000>;
				m25p,chunked-io = <32>;

				partition@0 {
					label = "u-boot";
					reg = <0x0 0x30000>;
					read-only;
				};

				partition@30000 {
					label = "u-boot-env";
					reg = <0x30000 0x10000>;
					read-only;
				};

				factory: partition@40000 {
					label = "factory";
					reg = <0x40000 0x10000>;
					read-only;
				};

				partition@50000 {
					label = "firmware";
					reg = <0x50000 0xf60000>;
				};

				partition@fb0000 {
					label = "user-config";
					reg = <0xfb0000 0x50000>;
				};
			};
		};
	};

	pcie@10140000 {
		status = "okay";
	};
};
